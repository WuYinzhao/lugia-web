{
  "widgetName": "Table",
  "title": "表格",
  "aliasName": null,
  "desc": "Table  表格。",
  "tag": "表格 tabel",
  "needExport": true,
  "props": {
    "lugiaHidden": {
      "desc": "是否渲染当前组件",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "columns": {
      "desc": "表格每一行展示的内容",
      "type": "object[]",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": [
        {
          "title": "Name",
          "dataIndex": "name",
          "key": "name"
        },
        {
          "title": "Age",
          "dataIndex": "age",
          "key": "age"
        },
        {
          "title": "Address",
          "dataIndex": "address",
          "key": "address"
        }
      ],
      "meta": [
        {
          "key": "title",
          "title": "表头文本",
          "type": "string"
        },
        {
          "key": "key",
          "title": "对应key",
          "type": "any"
        },
        {
          "key": "dataIndex",
          "title": "指定展示项",
          "type": "any"
        },
        {
          "key": "width",
          "title": "列宽",
          "type": "number"
        }
      ]
    },
    "selectOptions": {
      "desc": "表格是否有选中项",
      "type": "object",
      "designOnly": false,
      "injectProps": false,
      "meta": [
        {
          "key": "width",
          "title": "选择框所在列的宽度",
          "type": "number | string"
        },
        {
          "key": "selectRowKeys",
          "title": "指定选中行的key值集合",
          "type": "string[]"
        }
      ]
    },
    "data": {
      "desc": "数据源，指定 table 组件的数据",
      "type": "object[]",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": [
        {
          "name": "Jack",
          "age": 28,
          "address": "some where"
        },
        {
          "name": "Rose",
          "age": 36,
          "address": "some where"
        },
        {
          "name": "Bob",
          "age": 25,
          "address": "some where"
        }
      ],
      "meta": ""
    },
    "defaultExpandedRowKeys": {
      "desc": "初始展开的行的集合",
      "type": "string[]",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "expandedRowKeys": {
      "desc": "展开的行的集合",
      "type": "string[]",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "defaultExpandAllRows": {
      "desc": "初始是否展开所有的行",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "expandedRowRender": {
      "desc": "额外的展开行",
      "type": "function(recode, index, indent, expanded):reactnode",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "useFixedHeader": {
      "desc": "固定 Table 标题，使其固定在最上方",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "footer": {
      "desc": "表格尾部",
      "type": "function(currentpagedata)",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "indentSize": {
      "desc": "展示树形数据时，每层缩进的宽度，以 px 为单位",
      "type": "number",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": 15,
      "meta": ""
    },
    "rowKey": {
      "desc": "表格行 key 的取值，可以是字符串或一个函数",
      "type": "{ x: number | true, y: number }",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": "key",
      "meta": ""
    },
    "showHeader": {
      "desc": "是否展示标头",
      "type": "boolean",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": true,
      "meta": ""
    },
    "title": {
      "desc": "表格标题",
      "type": "function(currentpagedata)",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "size": {
      "desc": "设置表格单元格大小，可选 default、large、small，默认为 default;",
      "type": "sizetype",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "onHeaderRow": {
      "desc": "设置头部行属性",
      "type": "function(column, index)",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "onRow": {
      "desc": "设置行属性",
      "type": "function(record, index)",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "tableStyle": {
      "desc": "表格标题",
      "type": "tablestyletype",
      "designOnly": false,
      "injectProps": false,
      "defaultValue": "bordered",
      "meta": ""
    },
    "scroll": {
      "desc": "设置滚动条，也可用于指定滚动区域的宽和高",
      "type": "object",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "rowClassName": {
      "desc": "表格行的类名",
      "type": "function(record, index): string",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "collapseIcon": {
      "desc": "折叠行图标",
      "type": "icon | Function",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    },
    "expandIcon": {
      "desc": "展开行图标",
      "type": "icon | Function",
      "designOnly": false,
      "injectProps": false,
      "meta": ""
    }
  },
  "events": {
    "onExpandedRowsChange": {
      "desc": "展开的行改变时的回调",
      "args": [
        {
          "name": "expandedRows",
          "desc": "当前展开的行",
          "type": "string[]"
        }
      ]
    },
    "onExpand": {
      "desc": "点击展开图标时触发",
      "args": [
        {
          "name": "expanded",
          "desc": "当前是否展开",
          "type": "boolean"
        },
        {
          "name": "record",
          "desc": "当前行数据",
          "type": "Object"
        }
      ]
    },
    "onChange": {
      "desc": "点击排序变化时触发",
      "args": [
        {
          "name": "column",
          "desc": "当前列的表头数据",
          "type": "Object"
        },
        {
          "name": "filed",
          "desc": "当前列数据在数据项中对应的key",
          "type": "String"
        },
        {
          "name": "order",
          "desc": "当前排序类型",
          "type": "String"
        },
        {
          "name": "data",
          "desc": "当前排完序的数据",
          "type": "Array"
        }
      ]
    }
  },
  "designInfo": {},
  "category": [
    "数据展示"
  ],
  "type": {
    "tableStyleType": [
      "zebraStripe",
      "linear",
      "bordered"
    ],
    "SizeType": [
      "default",
      "large",
      "small"
    ]
  },
  "theme": {
    "Container": {
      "name": "表格整体样式",
      "desc": "为表格配置整体样式",
      "normal": [
        [
          "width"
        ],
        [
          "height"
        ]
      ]
    },
    "ExpandIcon": {
      "name": "展开行图标样式",
      "desc": "展开行图标样式配置点",
      "normal": [
        [
          "color"
        ],
        [
          "margin"
        ],
        [
          "fontSize"
        ],
        [
          "font"
        ],
        [
          "cursor"
        ]
      ],
      "hover": [
        [
          "color"
        ],
        [
          "margin"
        ],
        [
          "cursor"
        ]
      ],
      "active": [
        [
          "color"
        ],
        [
          "cursor"
        ],
        [
          "fontSize"
        ],
        [
          "font"
        ]
      ],
      "disabled": [
        [
          "color"
        ],
        [
          "cursor"
        ]
      ]
    },
    "CollapseIcon": {
      "name": "折叠行图标样式",
      "desc": "折叠行图标样式配置点",
      "normal": [
        [
          "color"
        ],
        [
          "margin"
        ],
        [
          "fontSize"
        ],
        [
          "font"
        ],
        [
          "cursor"
        ]
      ],
      "hover": [
        [
          "color"
        ],
        [
          "margin"
        ],
        [
          "cursor"
        ]
      ],
      "active": [
        [
          "color"
        ],
        [
          "cursor"
        ],
        [
          "fontSize"
        ],
        [
          "font"
        ]
      ],
      "disabled": [
        [
          "color"
        ],
        [
          "cursor"
        ]
      ]
    }
  },
  "childrenWidget": [
    "column",
    "selectOptions"
  ]
}
